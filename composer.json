{
    "name": "drupal/ckeditor_templates",
    "description": "Integrates the CKEditor templates plugin.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "keywords": ["Drupal"],
    "authors": [
        {
            "name": "Lucas Le Goff (lucaslg)",
            "homepage": "https://www.drupal.org/u/lucaslg",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://www.drupal.org/project/ckeditor_templates",
    "support": {
        "issues": "https://www.drupal.org/project/issues/ckeditor_templates"
    },
    "require": {
        "ckeditor-plugin/templates": "~4.14.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/templates",
                "version": "4.14.1",
                "type": "drupal-library",
                "vendor": "ckeditor-plugin",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.14.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    ]
}
